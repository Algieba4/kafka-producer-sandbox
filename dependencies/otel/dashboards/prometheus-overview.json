{
  "id": null,
  "title": "Prometheus Metrics Overview",
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "CPU Usage",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "process_cpu_seconds_total", "legendFormat": "CPU Seconds" }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Memory Usage",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "process_resident_memory_bytes", "legendFormat": "Memory Bytes" }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Request Rate",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "rate(http_server_requests_seconds_count[1m])", "legendFormat": "req/sec" }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 }
    }
  ]
}
